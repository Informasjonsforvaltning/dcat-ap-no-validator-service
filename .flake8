[flake8]
select = ANN,A50,B,B9,BLK,C,D,E,F,I,S,W,N
ignore = ANN101,ANN401,E501,W503
max-line-length = 120
per-file-ignores =
  __init__.py: F401,
  tests/*: S101,
  dcat_ap_no_validator_service/adapter/remote_graph_adapter.py: S110,
  gunicorn_config.py:B026
application-import-names = dcat_ap_no_validator_service, tests
import-order-style = google
